{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "downlevelIteration": true,
    "strictNullChecks": true,
    "noEmit": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ]
}
